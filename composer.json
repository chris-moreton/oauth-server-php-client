{
    "name": "chris-moreton/oauth-server-php-client",
    "description": "API Wrapper for the Netsensia authentication server",
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "dev-master",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {"Netsensia\\Authentication\\Api\\Client\\": "src/Netsensia/Authentication/Api/Client/"}
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/chris-moreton/guzzle"
        }
    ]
}
